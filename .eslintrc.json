{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
		"google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "plugins": [
		"import",
		"node",
		"promise",
        "react"
    ],
    "rules": {
		"require-jsdoc" : 0,
		"max-len": 0,
		"no-undef": 0
    }
}